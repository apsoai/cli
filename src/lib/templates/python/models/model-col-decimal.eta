    <%= it.field.name %>: Mapped[<% if (it.field.nullable !== false) { %>Optional[Decimal]<% } else { %>Decimal<% } %>] = mapped_column(
        Numeric(precision=<%= it.field.precision || 10 %>, scale=<%= it.field.scale || 2 %>),
        nullable=<%= it.field.nullable === false ? 'False' : 'True' %>
    )