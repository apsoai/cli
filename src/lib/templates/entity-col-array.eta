<% const {field, isGql} = it; %>


<% if (isGql === true) {%>
@Field(() => [String], { nullable: <%= Boolean(field.nullable) %> })
<% } %>
<% else {%>
    @Column("text", {
        array: true,
        nullable: <%= Boolean(field.nullable) %>
    })
    <% if (field.index ) {%>
    @Index()
    <% } %>
<% } %>

<%= field.name %>!: <%= field.dataType %>;