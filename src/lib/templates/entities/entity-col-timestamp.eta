<% const {field} = it; %>
<% if (field.name === 'createdAt') { %>
@CreateDateColumn()
createdAt: Date;
<% } else if (field.name === 'updatedAt') { %>
@UpdateDateColumn()
updatedAt: Date;
<% } else { %>
@Column({ type: 'timestamp', nullable: <%= Boolean(field.nullable) %> })
<%= field.name %>: Date;
<% } %>