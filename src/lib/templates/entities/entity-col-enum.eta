<% const {field} = it; %>

@Column({
    type: 'enum',
    enum: enums.<%= field.dataType%>,
    default: enums.<%= field.dataType%>.<%= field.default.toLowerCase().replace(/[^a-z0-9]/g, '_').replace(/^(\d)/, '_$1').replace(/_+/g, '_').replace(/_$/, '') %>
})
<% if (field.index ) {%>
@Index()
<% } %>

<%= field.name %>!: enums.<%= field.dataType%>;
