<% const {field} = it; %>
<% if (field.name === 'created_at') { %>
@CreateDateColumn()
created_at: Date;
<% } else if (field.name === 'updated_at') { %>
@UpdateDateColumn()
updated_at: Date;
<% } else { %>
@Column({ type: 'timestamptz', nullable: <%= Boolean(field.nullable) %> })
<%= field.name %>: Date;
<% } %>