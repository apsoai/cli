<% const {field, isGql} = it; %>

<% if (isGql === true) {%>
   @Field(() => enums.<%= field.dataType%>)
<% } %>
<% else {%>
    @Column({
        type: 'enum',
        enum: enums.<%= field.dataType%>,
        default: enums.<%= field.dataType%>.<%= field.default %>
    })
    <% if (field.index ) {%>
    @Index()
    <% } %>
<% } %>

<%= field.name %>!: enums.<%= field.dataType%>;