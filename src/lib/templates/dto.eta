<%~ includeFile('./header.eta') %>

<% it.enumTypes.forEach((enumType) => { %>
enum <%= enumType.name %> {
<%~ enumType.values.map((enumValue) => `${enumValue} = '${enumValue}'`).join(', ') %>
}
<% }) %>

export class <%= it.name %>Dto {
<% it.columns.forEach((field) => { %>
  <%= field.name %>: <%= field.dataType %>;
<% }) %>
};