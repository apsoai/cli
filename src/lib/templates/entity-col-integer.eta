<% const {field, isGql} = it; %>

<% if (isGql === true) {%>
    @Field(() => Number <% if (field.default !== null) {%>, { nullable: true } <% }%>)
<% } %>
<% else {%>
    @Column({ "type": "int"<% if (field.default !== null) {%>, default:  <%= field.default %><% } %> })
    <% if (field.index ) {%>
    @Index()
    <% } %>
    <% if (field.primary ) {%>
    @PrimaryColumn()
    <% } %>
<% } %>

<%= field.name %>: <%= field.dataType %>;