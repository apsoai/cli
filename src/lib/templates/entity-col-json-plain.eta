<% const {field, isGql} = it; %>

<% if (isGql === true) {%>
    @Field(() => JSONResolver, { nullable: <%= Boolean(field.nullable) %> })
<% } %>
<% else {%>
    @Column('json',{ nullable: <%= Boolean(field.nullable) %>  })
    <% if (field.index ) {%>
    @Index()
    <% } %>
<% } %>

<%= field.name %>: JSON;