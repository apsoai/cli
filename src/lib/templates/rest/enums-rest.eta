<%~ includeFile('./header.eta') %>

<% it.allEnumsFields.forEach((enumType) => { %>
export enum <%= enumType.name %> {
<% for (const enumValue of (enumType.values || [])) { %>
  <%= enumValue.toLowerCase().replace(/[^a-z0-9]/g, '_').replace(/^(\d)/, '_$1').replace(/_+/g, '_').replace(/_$/, '') %> = '<%= enumValue %>',
<% } %>
}
<% }) %>